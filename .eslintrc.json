{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
        "incident": ["error", 1],
        "semi": ["error", "always"],
        "quotes": ["error", "single"]
    }
}
